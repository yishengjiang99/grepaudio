{
	"version": "0.2.0",
	"configurations": [
	{
		"name": "Attach to Chrome",
		"port": 9222,
		"request": "attach",
		"type": "pwa-chrome",
		"webRoot": "${workspaceFolder}"
	},
		{
			"request": "launch",
			"program": "${command:AskForProgramName}",
			"cwb": "${workspaceFolder}",
			"smartclientBin": "C:/totvs/bin/smartclient/smartclient.exe",
			"isMultiSession": true,
			"enableTableSync": true
		}
	]
}